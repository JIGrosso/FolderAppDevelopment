    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:FolderApp.Views"
    Visual="Material">

    <NavigationPage.TitleView>
        <Label Style="{StaticResource TitleLabel}" Text="{Binding TitleText}" />
    </NavigationPage.TitleView>


    <ContentPage.Content>
        <views:PostList />
    </ContentPage.Content>
</ContentPage>